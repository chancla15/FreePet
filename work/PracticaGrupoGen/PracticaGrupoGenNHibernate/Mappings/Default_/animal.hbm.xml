<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="PracticaGrupoGenNHibernate"
		                   namespace="PracticaGrupoGenNHibernate.EN.Default_">
			<class name="AnimalEN" table="[Animal]">
						<id name="Microchip" column="[microchip]"/>
					<property name="Name" type="string"> 
						<column name="[name]"  not-null="true"/>
					</property>
					<property name="Raza" > 
						<column name="[raza]"  not-null="true"/>
					</property>
					<property name="Tamanyo" > 
						<column name="[tamanyo]"  not-null="true"/>
					</property>
					<property name="Peso" type="double"> 
						<column name="[peso]"  not-null="true"/>
					</property>
					<property name="Fechanac" type="date"> 
						<column name="[fechanac]"  not-null="true"/>
					</property>
					<property name="Sexo" type="character"> 
						<column name="[sexo]"  not-null="true"/>
					</property>
			
		  			
						<bag name="Cliente" >
							<key>
								<column name="[FK_microchip_animal]"/>
					      	</key>
							<one-to-many class="PracticaGrupoGenNHibernate.EN.Default_.ClienteEN"/>
					    </bag>
					
			
			
			
			
			
			
			
					
					
					
					
					
					
					
					
		
		  </class>
		  
		  <query name="AnimalENReadAllHQL">from AnimalEN</query>
		</hibernate-mapping>
	